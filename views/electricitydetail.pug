extends layout
block content
 h1= title
 if (toShow == null)
    h2 No instance to details 
 else
  - let electricityid = toShow._id
  - let Edistributor_Name = toShow.Edistributor_Name
  - let electricity_gen = toShow.electricity_gen
  - let elctricity_type = toShow.elctricity_type
  div
   div.row
    div(class='electricityAttr mr-3') Electricity ID is:
    div #{electricityid}
   div.row
    div(class='electricityAttr mr-3') Electricity Name is:
    div #{Edistributor_Name}
   div.row
    div(class='electricityAttr mr-3') Electricity Generated by:
    div #{electricity_gen}
   div.row
    div(class='electricityAttr mr-3') Electricity Type is:
    div #{elctricity_type}

  

